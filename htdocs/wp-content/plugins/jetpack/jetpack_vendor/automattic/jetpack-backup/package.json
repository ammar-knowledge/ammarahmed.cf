{
	"private": true,
	"description": "Easily restore or download a backup of your site from a specific moment in time.",
	"homepage": "https://github.com/Automattic/jetpack/tree/HEAD/projects/packages/backup/#readme",
	"bugs": {
		"url": "https://github.com/Automattic/jetpack/labels/[Package] Backup"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Automattic/jetpack.git",
		"directory": "projects/packages/backup"
	},
	"license": "GPL-2.0-or-later",
	"author": "Automattic",
	"scripts": {
		"build": "pnpm run clean && pnpm run build-client",
		"build-client": "webpack",
		"build-concurrently": "pnpm run clean && concurrently 'pnpm:build-client' 'pnpm:build-php'",
		"build-production-concurrently": "pnpm run clean && concurrently 'NODE_ENV=production BABEL_ENV=production pnpm run build-client' && pnpm run validate",
		"clean": "rm -rf build/",
		"test": "jest --config=tests/jest.config.js",
		"validate": "pnpm exec validate-es build/",
		"watch": "pnpm run build && webpack watch"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"dependencies": {
		"@automattic/components": "2.1.0",
		"@automattic/format-currency": "2.0.0",
		"@automattic/jetpack-analytics": "workspace:*",
		"@automattic/jetpack-api": "workspace:*",
		"@automattic/jetpack-components": "^0.53.0",
		"@automattic/jetpack-connection": "workspace:*",
		"@wordpress/api-fetch": "6.55.0",
		"@wordpress/data": "9.28.0",
		"@wordpress/date": "4.58.0",
		"@wordpress/element": "5.35.0",
		"@wordpress/i18n": "4.58.0",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@automattic/jetpack-base-styles": "^0.6.4",
		"@automattic/jetpack-webpack-config": "workspace:*",
		"@babel/core": "7.24.0",
		"@babel/preset-env": "7.24.0",
		"@babel/runtime": "7.24.0",
		"@testing-library/dom": "9.3.4",
		"@testing-library/react": "14.2.1",
		"@testing-library/user-event": "14.5.2",
		"@wordpress/browserslist-config": "5.41.0",
		"concurrently": "8.2.2",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"sass": "1.77.4",
		"sass-loader": "13.3.3",
		"webpack": "5.92.0",
		"webpack-cli": "5.1.4"
	}
}
